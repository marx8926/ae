<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DiscipuladoClase" table="discipulado_clase">
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="idClaseCell" type="bigint" column="id_clase_cell" nullable="false"/>
    <field name="idMiembro" type="bigint" column="id_miembro" nullable="false"/>
    <field name="estado" type="boolean" column="estado" nullable="true"/>
    <field name="fechaDicto" type="date" column="fecha_dicto" nullable="true"/>
    <field name="asistio" type="boolean" column="asistio" nullable="true"/>
    <field name="dictado" type="date" column="dictado" nullable="true"/>
    <many-to-one field="idClaseCelula" target-entity="ClaseCell">
      <join-columns>
        <join-column name="id_clase_celula" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDiscipulado" target-entity="Discipulado">
      <join-columns>
        <join-column name="id_discipulado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
